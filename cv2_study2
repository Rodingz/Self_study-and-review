import cv2
img1 = cv2.imread("C:\Users\kevin\Desktop\python_selfst\img_f1.jpg", cv2.IMREAD_GRAYSCALE)
img2 = cv2.imread("C:\Users\kevin\Desktop\python_selfst\img_f1.jpg", cv2.IMREAD_ANYCOLOR)
